{
    "version": 2,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 20,
      "patch": 0
    },
    "configurePresets": [
      {
        "name": "CUDD-Z3",
        "description": "Configure with Ninja, metaSMT_USE_CUDD=ON, and metaSMT_USE_Z3=ON",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build/${presetName}",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Release",
          "metaSMT_USE_CUDD": "ON",
          "metaSMT_USE_Z3": "ON"
        }
      },
      {
        "name": "FREE",
        "inherits": "CUDD-Z3",
        "cacheVariables": {
          "metaSMT_USE_YICES2": "ON",
          "metaSMT_USE_Boolector": "OFF",
          "metaSMT_USE_CVC4": "OFF",
          "metaSMT_USE_STP": "OFF",
          "metaSMT_USE_Aiger": "OFF",
          "metaSMT_USE_picosat": "OFF"
        }
      }
    ]
}